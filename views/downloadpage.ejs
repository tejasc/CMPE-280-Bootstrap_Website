	<script type="text/javascript">
	
	  function downloadForm(){
		  
		  var json = {
				        "fname":document.getElementById('fname').value,
				        "lname":document.getElementById('lname').value, 
				        "email":document.getElementById('email').value
		  };
		  $.ajax({
			  url: '/downloadFormInfo',
			  type: 'POST',
			  data: JSON.stringify(json),
			  contentType: "application/json",
			  dataType: 'json',

			  })
			  .done(function(response) {
			  console.log("success");
			  alert(response);
			  })
			  .fail(function() {
			  console.log("error");
			  })
			  .always(function() {
			  console.log("complete");
			  });
		  window.open('http://www.undatarevolution.org/wp-content/uploads/2014/11/A-World-That-Counts.pdf');
		  //var download = document.getElementById("download");
		  //download.src = "http://www.planet-data.eu/sites/default/files/presentations/Big_Data_Tutorial_part4.pdf";
		}	
	
	</script>

